{
  "name": "linkorb/org-sync",
  "license": "MIT",
  "type": "library",
  "description": "Organization sync library",
  "require": {
    "php": "^7.3",
    "symfony/cache": "^4.3",
    "symfony/serializer": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "LinkORB\\OrgSync\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LinkORB\\OrgSync\\Tests\\": "tests/unit/"
    }
  }
}
